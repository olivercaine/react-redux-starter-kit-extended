{
    "extends": "tslint:recommended",
    "linterOptions": {
        "exclude": [
            "node_modules/**"
        ]
    },
    "rules": {
        "no-null-keyword": true,
        "semicolon": false,
        "quotemark": [
            true,
            "single",
            "avoid-escape"
        ]
    }
}
