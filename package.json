{
  "name": "react-redux-starter-kit",
  "version": "3.0.1",
  "description": "Get started with React, Redux, and React-Router!",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist",
    "dev": "concurrently \"npm run start\" \"npm run test:watch\" \"npm run storybook\"",
    "compile": "node build/scripts/compile",
    "build": "npm run clean && npm run build-storybook && cross-env NODE_ENV=production npm run compile && cp ./devops/static-web-app/* ./dist",
    "start": "cross-env NODE_ENV=development node build/scripts/start",
    "test": "cross-env NODE_ENV=test karma start build/karma.config",
    "test:watch": "npm test -- --watch",
    "lint": "tslint -p ./tsconfig.json && eslint .",
    "lint:fix": "npm run lint -- --fix",
    "storybook": "start-storybook -p 6006 -s ./public",
    "build-storybook": "build-storybook -o ./dist/storybook"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run lint && npm test"
    }
  },
  "engines": {
    "node": ">=8.0.0 <9"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/davezuko/react-redux-starter-kit.git"
  },
  "author": "David Zukowski <david@zuko.me> (http://zuko.me)",
  "license": "MIT",
  "dependencies": {
    "@common/fetch": "file:common/fetch",
    "@common/month-enum-ts": "file:common/month-enum-ts",
    "@common/reducers": "file:common/reducers",
    "bootstrap": "^4.3.1",
    "compression": "^1.6.2",
    "express": "^4.14.0",
    "object-assign": "^4.1.1",
    "promise": "^7.1.1",
    "prop-types": "^15.7.2",
    "react": "^16.8.3",
    "react-dom": "^16.8.3",
    "react-redux": "^5.0.4",
    "react-router": "^3.0.0",
    "redbox-react": "^1.3.6",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0",
    "whatwg-fetch": "^2.0.3"
  },
  "devDependencies": {
    "@storybook/react": "^3.0.1",
    "@types/chai": "^4.1.3",
    "@types/enzyme-adapter-react-16": "^1.0.5",
